[
  {
    "Id": 1,
    "name": "Email to Slack",
    "description": "Forward important emails to a Slack channel for team visibility",
    "category": "Communication",
    "popularity": 95,
    "estimatedTime": "2 min",
    "services": ["Gmail", "Slack"],
    "triggers": 1,
    "actions": 1,
    "workflow": {
      "name": "Email to Slack Notification",
      "description": "Forward important emails to Slack channel",
      "nodes": [
        {
          "id": "template-node-1",
          "type": "trigger",
          "service": "Gmail",
          "action": "New Email",
          "position": { "x": 100, "y": 200 },
          "config": {
            "criteria": "priority:high"
          }
        },
        {
          "id": "template-node-2",
          "type": "action", 
          "service": "Slack",
          "action": "Send Message",
          "position": { "x": 400, "y": 200 },
          "config": {
            "channel": "#notifications",
            "message": "New email from {{sender}}: {{subject}}"
          }
        }
      ],
      "connections": [
        {
          "id": "template-conn-1",
          "sourceNodeId": "template-node-1",
          "targetNodeId": "template-node-2"
        }
      ]
    }
  },
  {
    "Id": 2,
    "name": "Lead Generation",
    "description": "Automatically create CRM contacts from form submissions",
    "category": "Sales & Marketing",
    "popularity": 87,
    "estimatedTime": "3 min",
    "services": ["Typeform", "HubSpot"],
    "triggers": 1,
    "actions": 1,
    "workflow": {
      "name": "Form to CRM Lead",
      "description": "Convert form submissions to CRM contacts",
      "nodes": [
        {
          "id": "template-node-3",
          "type": "trigger",
          "service": "Typeform",
          "action": "Form Submitted",
          "position": { "x": 100, "y": 200 },
          "config": {
            "formId": "your-form-id"
          }
        },
        {
          "id": "template-node-4",
          "type": "action",
          "service": "HubSpot",
          "action": "Create Contact",
          "position": { "x": 400, "y": 200 },
          "config": {
            "firstName": "{{first_name}}",
            "lastName": "{{last_name}}",
            "email": "{{email}}",
            "company": "{{company}}"
          }
        }
      ],
      "connections": [
        {
          "id": "template-conn-2",
          "sourceNodeId": "template-node-3",
          "targetNodeId": "template-node-4"
        }
      ]
    }
  },
  {
    "Id": 3,
    "name": "Daily Reports",
    "description": "Generate and email daily business reports automatically",
    "category": "Analytics", 
    "popularity": 78,
    "estimatedTime": "5 min",
    "services": ["Schedule", "Google Sheets", "Gmail"],
    "triggers": 1,
    "actions": 2,
    "workflow": {
      "name": "Automated Daily Reports",
      "description": "Generate daily reports and email them to team",
      "nodes": [
        {
          "id": "template-node-5",
          "type": "trigger",
          "service": "Schedule",
          "action": "Daily at 8 AM",
          "position": { "x": 100, "y": 200 },
          "config": {
            "time": "08:00",
            "timezone": "America/New_York"
          }
        },
        {
          "id": "template-node-6",
          "type": "action",
          "service": "Google Sheets",
          "action": "Generate Report",
          "position": { "x": 300, "y": 200 },
          "config": {
            "spreadsheetId": "your-spreadsheet-id",
            "range": "A1:Z1000"
          }
        },
        {
          "id": "template-node-7",
          "type": "action",
          "service": "Gmail",
          "action": "Send Email",
          "position": { "x": 500, "y": 200 },
          "config": {
            "to": "team@company.com",
            "subject": "Daily Report - {{date}}",
            "body": "Daily business report attached"
          }
        }
      ],
      "connections": [
        {
          "id": "template-conn-3",
          "sourceNodeId": "template-node-5",
          "targetNodeId": "template-node-6"
        },
        {
          "id": "template-conn-4",
          "sourceNodeId": "template-node-6", 
          "targetNodeId": "template-node-7"
        }
      ]
    }
  },
